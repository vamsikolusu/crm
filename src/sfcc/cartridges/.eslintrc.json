{
    "root": true,
    "extends": "airbnb-base/legacy",
    "globals" : {
        "session": true,
        "request": true,
        "response": true,
        "customer": true,
        "dw": true,
        "empty": true,
        "webreferences": true,
        "webreferences2": true,
        "PIPELET_NEXT": true,
        "PIPELET_ERROR": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "camelcase": 0,
        "consistent-return": 0,
        "eqeqeq": 0,
        "func-names": 0,
        "function-paren-newline": 0,
        "global-require": 0,
        "guard-for-in": 0,
        "indent": [
            "error",
            4
        ],
        "linebreak-style": 0,
        "lines-around-directive": 0,
        "max-len": 0,
        "newline-per-chained-call": 0,
        "no-alert": 0,
        "no-console": 0,
        "no-empty": [
            "error",
            {
                "allowEmptyCatch": true
            }
        ],
        "no-fallthrough": [
            "error",
            {
                "commentPattern": "break[\\s\\w]*omitted"
            }
        ],
        "no-mixed-operators": 0,
        "no-multi-assign": 0,
        "no-new": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "no-prototype-builtins": 0,
        "no-restricted-globals": 0,
        "no-restricted-syntax": 0,
        "no-shadow": 0,
        "no-use-before-define": 0,
        "no-underscore-dangle": 0,
        "no-unused-expressions": 0,
        "one-var": 0,
        "padded-blocks": 0,
        "radix": 0,
        "vars-on-top": 0
    }
}




